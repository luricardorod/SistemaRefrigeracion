    .col-md-5.fixed-element
      table.table.table-hover
        tr
          th Salida
          th Valor
        tr
          td
            button.button.btn.btn-success( ng-click = "play()") Play
            button.button.btn.btn-danger( ng-click = "stop()") Stop
            button.btn.btn-primary( data-toggle = "modal",
              data-target="#myModal" ) Gráficas
          td( ng-bind = 'timer' )

        tr( ng-class = '{ "selected-class-name": $index == selectedIndex }' , 
          ng-repeat = 'output in outputs' )
          td( ng-bind = 'output.label' )
          td( ng-bind = 'output.value | number:4' )

    #myModal.modal.fade( tabindex='-1', role='dialog', 
      aria-labelledby='myModalLabel', aria-hidden='true')
      .modal-dialog
        .modal-content
          .modal-header
            button.close(type='button', data-dismiss='modal', aria-hidden='true') ×
            h4#myModalLabel.modal-title Gráficas
          div( google-chart, chart = 'char', style = '{{char.cssStyle}}',
            on-ready="chartReady()" )

block custom-scripts
  script( src='/javascripts/own/models.js' )
